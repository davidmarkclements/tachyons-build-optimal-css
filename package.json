    "postcss-class-repeat": "^0.1.0",
    "postcss-class-whitelist": "^1.0.1",
    "postcss-conditionals": "^2.0.2",
    "postcss-css-variables": "^0.5.2",
    "postcss-custom-media": "^5.0.1",
    "postcss-discard-comments": "^2.0.4",
    "postcss-import": "^8.1.2"
  },
  "devDependencies": {
